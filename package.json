{
  "name": "codetria-vite-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "panorama": "yarn start --host --mode panorama.dev",
    "grit": "yarn start --host --mode grit.dev",
    "build": "tsc && node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build",
    "preview": "vite preview",
    "deploy-grit-staging": "yarn build --mode grit.staging && aws s3 sync --cache-control 'no-cache' dist/ s3://web-grit-staging  --profile palier && aws cloudfront create-invalidation --distribution-id E2S2WS61XANB3J --path /* --profile palier",
    "deploy-grit-dev": "yarn build --mode grit.dev && aws s3 sync --cache-control 'no-cache' dist/ s3://web-grit  --profile palier && aws cloudfront create-invalidation --distribution-id E33MVEBKMMOYL5 --path /* --profile palier",
    "deploy-panorama-dev": "yarn build --mode panorama.dev && aws s3 sync --cache-control 'no-cache' dist/ s3://web-palier  --profile palier && aws cloudfront create-invalidation --distribution-id E30NOKUL8YY2CF --path /* --profile palier",
    "deploy-panorama-staging": "yarn build --mode panorama.staging && aws s3 sync --cache-control 'no-cache' dist/ s3://web-palier-staging  --profile palier && aws cloudfront create-invalidation --distribution-id E3CWT3YTMBV1M9 --path /* --profile palier",
    "deploy-dev": "yarn run deploy-panorama-dev && yarn run deploy-grit-dev",
    "deploy-staging": "yarn run deploy-panorama-staging && yarn run deploy-grit-staging",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx,.json --fix",
    "format": "prettier \"./src/**/*.{ts,tsx,js,jsx,css,scss}\" --write",
    "compile": "tsc",
    "prepare": "husky install",
    "mock:server": "json-server --watch ./mock/index.json --config ./mock/config.json"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.88",
    "@mui/material": "^5.8.6",
    "@mui/utils": "^5.9.3",
    "@mui/x-date-pickers": "^5.0.2",
    "@react-pdf-viewer/core": "3.7.0",
    "@react-pdf-viewer/default-layout": "^3.7.0",
    "@reduxjs/toolkit": "^1.8.2",
    "@tanstack/react-query": "^4.3.9",
    "@tanstack/react-query-devtools": "^4.11.0",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-helmet": "^6.1.5",
    "axios": "^0.27.2",
    "date-fns": "^2.29.2",
    "dayjs": "^1.11.5",
    "dompurify": "^2.4.0",
    "formik": "^2.2.9",
    "html-react-parser": "^3.0.4",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "jodit-react": "^1.3.23",
    "lint-staged": "^13.0.3",
    "moment": "^2.29.4",
    "pdfjs-dist": "2.15.349",
    "react": "^18.0.0",
    "react-data-table-component": "^7.5.3",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^11.18.5",
    "react-lottie-player": "^1.4.3",
    "react-moment": "^1.1.2",
    "react-router-dom": "6",
    "react-select": "^5.4.0",
    "react-webcam": "^7.0.1",
    "styled-components": "^5.3.5",
    "vite-plugin-ejs": "^1.6.4",
    "vite-plugin-svgr": "^2.2.1",
    "xlsx": "^0.18.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/dompurify": "^2.3.4",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "json-server": "^0.17.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  }
}